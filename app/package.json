{
  "name": "app",
  "version": "1.0.0",
  "description": "Chess web application",
  "scripts": {
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|scss|md)\"",
    "start": "webpack-dev-server --env.development",
    "build": "webpack --env.production",
    "test": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\""
